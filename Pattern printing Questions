public class Pattern {
    public static void main(String[] args) {
for(int i = 1; i <= 4; i++) {      // Outer loop â†’ rows
            for(int j = 1; j <= 4; j++) {  // Inner loop â†’ columns
                System.out.print("* ");
            }
            System.out.println(); // Move to next line
        }
    }
}


